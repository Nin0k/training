@using PL.Web
@{
    var WebUserPL = new WebUsersPL();
    var WebAwardPL = new WebAwardsPL();

    var type = Request["type"];
   
    if (type == "user")
    {
        var name = Request["name"];
        var birthday = Request["birthday"];
        var newUser = WebUserPL.AddUser(name, birthday);
    }
    else if (type == "award")
    {
        var title = Request["title"];

        var newUser = WebAwardPL.AddAward(title);
    }
}
